package figura;

public class Circulo extends Figura{
	private Double radio;
	public Circulo(double r) {
		this.radio = r;
	}
	public void imprirmir () {
		System.out.println("Radio del circulo "  + radio);
	}
	public Double getRadio() {
		return radio;
	}
	public void setRadio(Double radio) {
		this.radio = radio;
	}
	
}
